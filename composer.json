{
    "name": "rekalogika/direct-property-access",
    "description": "Simple implementation of PropertyAccessorInterface that reads and writes directly to the object's properties, bypassing getters and setters.",
    "homepage": "https://rekalogika.dev/direct-property-access",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rekalogika\\DirectPropertyAccess\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rekalogika\\DirectPropertyAccess\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Priyadi Iman Nurcahyo",
            "email": "priyadi@rekalogika.com"
        }
    ],
    "require": {
        "symfony/property-access": "^6.1 || ^7.0"
    },
    "require-dev": {
        "symfony/config": "^6.1 || ^7.0",
        "symfony/dependency-injection": "^6.1 || ^7.0",
        "symfony/http-kernel": "^6.1 || ^7.0",
        "phpstan/phpstan": "^1.12",
        "phpunit/phpunit": "^10.5",
        "vimeo/psalm": "^5.26",
        "psalm/plugin-phpunit": "^0.19.0",
        "symfony/phpunit-bridge": "^6.1 || ^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.2-dev"
        }
    }
}
